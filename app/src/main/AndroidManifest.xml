        <activity android:name=".activities.admin_activites.AdminMainActivity" />
        <activity android:name=".activities.admin_activites.AdminDetailHotelActivity" />
        <activity android:name=".activities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.SetNewPassword" />
        <activity android:name=".activities.ResetPasswordActivity" />
        <activity android:name=".activities.SignUpActivity" />
        <activity android:name=".activities.SignInActivity" />
        <activity android:name=".activities.user_activities.UserMainActivity" />
